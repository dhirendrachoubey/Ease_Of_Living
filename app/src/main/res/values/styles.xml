<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">

        <item name="colorPrimary">@color/colorTitleBar</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
   <!-- <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">

        <item name="colorPrimary">@color/colorTitleBar</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>-->
    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="homeAsUpIndicator">@color/blue</item>
        <item name="spinBars">true</item>
        <item name="color">@color/blue</item>
    </style>
    <style name="buttonBlue" parent="@android:style/Widget.Button">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">250dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/button_main</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="buttonLightBlue" parent="@android:style/Widget.Button">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">250dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/button_secondary</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="buttonSaveAndProceed" parent="@android:style/Widget.Button">
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:padding">5dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:text">@string/save_proceed</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:background">@color/colorPrimary</item>

    </style>

    <!--Layout Width and Height-->
    <style name="layoutMatchParent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="layoutWrapContent">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="layoutMatchWidthWrapHeight" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- Linear layout -->
    <style name="linearLayoutVertical1" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:visibility">visible</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="linearLayoutVertical2" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:visibility">gone</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="linearLayoutHorizontal1" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:visibility">visible</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="linearLayoutHorizontal2" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:visibility">gone</item>
        <item name="android:background">@drawable/et_style</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="linearLayoutVertical3" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/et_style</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="linearLayoutVertical3WithMarginBottom" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/et_style</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
    </style>

    <style name="linearLayoutVertical3WithMarginBottomGone" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/et_style</item>
        <item name="android:visibility">gone</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
    </style>

    <style name="linearLayoutHorizontal3" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:visibility">visible</item>
        <item name="android:background">@drawable/et_style</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="linearLayoutHorizontal3WithMarginBottom" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:visibility">visible</item>
        <item name="android:background">@drawable/et_style</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="linearLayoutHorizontal4" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:visibility">visible</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingTop">5dp</item>
    </style>

    <style name="linearLayoutHorizontal5" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:visibility">visible</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- Text view -->
    <style name="textView1" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="textView2" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="textView3" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center|left</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="textView4" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">15sp</item>
        <item name="android:visibility">gone</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="textViewGreyLine" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:background">@android:color/darker_gray</item>
    </style>



    <style name="textView6" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:visibility">gone</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">italic</item>
    </style>

    <style name="textView2WithMarginBottom" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="textView8" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">italic</item>
    </style>

    <style name="textView8WithMarginBottom" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">italic</item>
        <item name="android:layout_marginBottom">8dp</item>
    </style>

    <style name="textViewClickable" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">italic</item>
    </style>

    <style name="textViewItalic" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textStyle">italic</item>
    </style>

    <!-- Edit Text -->
    <style name="editText1" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/et_style</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="editText2" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/et_style</item>
        <item name="android:textSize">15sp</item>
        <item name="android:enabled">false</item>

    </style>


    <style name="editText3" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:textSize">15sp</item>
        <item name="android:enabled">false</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:focusable">false</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>


    <style name="editText1WithMarginBottom" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/et_style</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="hhd_food_grain_fps">Total no of households receiving food grains from FPS</style>

</resources>
